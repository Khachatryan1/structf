struct Human{
    int age;
    double height;
    std::string name;
};
void print(Human a){
    cout << a.age << "\n"<< a.name << "\n"<< a.height << "\n";
}
int main() {
//    Dog dog("jeko",10);
//    int age = dog.print();
    Human jhon;
    jhon.height = 15.2;
    jhon.name = "jhon";
    jhon.age = 15;
    print(jhon);
    return 0;
}
